  "source": "pragma solidity ^0.5.0;\n\ncontract Ownable {\n  \n  address public owner;\n\n  constructor(address initialOwner) public{\n    owner = initialOwner;\n  }\n\n  modifier onlyOwner() {\n    require(msg.sender == owner, \"Error, only owner\");\n    _;\n  }\n}",
